{
    "geode": "5.0.0-beta.2",
    "gd": {
        "android": "2.2081",
        "win": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
    },
    "version": "v2.0.0",
    "id": "hiimjustin000.integrated_demonlist",
    "name": "Integrated Demonlist & Stats",
    "developers": ["hiimjasmine00", "ItsMochaTheOtter", "TechStudent10"],
    "description": "AREDL, Pemonlist, and GSV integration",
    "resources": {
        "files": [
            "resources/*.frag",
            "resources/*.vert",
            "resources/profile-rankings/*.plist"
        ],
        "sprites": [
            "resources/*.png",
            "resources/gsv/*.png"
        ],
        "spritesheets": {
            "GSV_Flags": [
                "resources/gsv/flags/*.png",
                "resources/gsv/flags/countries/*.png",
                "resources/gsv/flags/regions/*.png",
                "resources/gsv/flags/subdivisions/au/*.png",
                "resources/gsv/flags/subdivisions/cn/*.png",
                "resources/gsv/flags/subdivisions/dk/*.png",
                "resources/gsv/flags/subdivisions/fi/*.png",
                "resources/gsv/flags/subdivisions/fr/*.png",
                "resources/gsv/flags/subdivisions/gb/*.png",
                "resources/gsv/flags/subdivisions/nl/*.png",
                "resources/gsv/flags/subdivisions/no/*.png",
                "resources/gsv/flags/subdivisions/us/*.png"
            ],
            "PR_Sprites": [
                "resources/profile-rankings/*.png"
            ]
        }
    },
    "settings": {
        "enable-rank": {
            "name": "Enable Demon Ranking Text",
            "description": "Whether or not to display the demon ranking text.",
            "type": "bool",
            "default": true
        },
        "white-rank": {
            "name": "White Demon Ranking Text",
            "description": "Whether or not to display the demon ranking text in white.",
            "type": "bool",
            "enable-if": "enable-rank",
            "default": false
        }
    },
    "dependencies": {
        "geode.node-ids": ">=v1.9.0"
    },
    "links": {
        "community": "https://discord.gg/QVKmbvBXA7",
        "source": "https://github.com/hiimjasmine00/IntegratedDemonlist",
        "homepage": "https://www.hiimjasmine00.com"
    },
    "tags": [
        "content",
        "enhancement",
        "interface",
        "online",
        "utility"
    ]
}
